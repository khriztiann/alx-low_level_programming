#include "main.h"
#include <stdio.h>

/**
 * print_array - the print which is used to check the code
 * @a: an integer
 * @n: the element
 * Return: print code
 */

void print_array(int *a, int n)
{
	int i;

	i = 0;
	while (i < n)
	{
		if (i != 0)
		{
			printf(", ");
		}
		printf("%d", a[i]);
		i++;
	}
	printf("\n");
}
